<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 xmlns="urn:nhibernate-mapping-2.2"
 namespace="ExamSheet.Repository.Semester"
 assembly="ExamSheet.Repository">
  
  <class name="Semester" table="Semesters">
    <id name="Id" column="Id" type="string"></id>
    <property name="Year" column="Year" type="short" />
    <property name="Number" column="Number" type="short" />
  </class>
  
</hibernate-mapping>