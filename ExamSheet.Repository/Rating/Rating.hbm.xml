<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 xmlns="urn:nhibernate-mapping-2.2"
 namespace="ExamSheet.Repository.Rating"
 assembly="ExamSheet.Repository">
  
  <class name="Rating" table="Ratings">
    <composite-id>
      <key-property name="StudentId" column="StudentId" type="string" />
      <key-property name="ExamSheetId" column="ExamSheetId" type="string" />
    </composite-id>
    <property name="Mark" column="Mark" type="short" />
  </class>
  
</hibernate-mapping>